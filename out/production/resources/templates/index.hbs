{{#partial "content"}}
     {{#if animals}}
         {{#each animals}}
            <ul>
                <li>{{name}}</li>
                <li>{{gender}}</li>
                <li>{{type}}</li>
                <li>{{breed}}</li>
            </ul>
            <a href="/animals/{{id}}">read more...</a>
         {{/each}}
     {{/if}}
{{/partial}}
{{>layout.hbs}}